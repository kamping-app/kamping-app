{
  "name": "kamping-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run tailwind:css && vue-cli-service serve",
    "build": "npm run tailwind:css && vue-cli-service build && npm run purgecss",
    "lint": "vue-cli-service lint",
    "cy:run": "cypress run",
    "purgecss": "purgecss -c purgecss.config.js -o dist/css/",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "tailwind:css": "./node_modules/.bin/tailwind build ./src/assets/css/main.css -c ./tailwind.config.js -o ./public/css/styles.css"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-i18n": "^8.11.2",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.0 || ^5.0.0",
    "@storybook/addon-knobs": "^4.1.0 || ^5.0.0",
    "@storybook/addon-links": "^4.1.0 || ^5.0.0",
    "@storybook/addon-notes": "^4.1.0 || ^5.0.0",
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "cypress": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.8.0",
    "purgecss": "^1.3.0",
    "tailwindcss": "1.0.0-beta.5",
    "vue-jest": "^3.0.4",
    "vue-cli-plugin-storybook": "^0.6.1",
    "vue-template-compiler": "^2.5.21"
  }
}
